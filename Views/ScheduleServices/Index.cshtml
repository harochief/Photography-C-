@model IEnumerable<Identity4Example.Models.ScheduleService>

@{
    ViewBag.Title = "Indexe";
}

@{
    Layout = "~/Views/Shared/_layout3.cshtml";
}

<h3><span class="green">Messages From </span><span class="green1">Quick Contact</span></h3>
<br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InterestOfService)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Message)
        </th>
        <th>Date Submitted</th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InterestOfService)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Message)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.GetDate)
        </td>
        <td>
        
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
