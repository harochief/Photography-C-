

<!DOCTYPE html>
<html lang="zxx">
<head>
    <title>@ViewBag.Title - Tembisa Property Stokvel</title>


    <meta charset="utf-8" />
    <meta name="author" content="Softnio">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" href="image/favicon.png" type="image/png" sizes="16x16">
    <link rel="stylesheet" type="text/css" href="css/vendor.bundle.css">
    <link id="style-css" rel="stylesheet" type="text/css" href="css/style.css">
 
    @Styles.Render("~/css/vendor.bundle.css")
    @Styles.Render("~/css/style-GreenRed.css")


</head>
<body class="site-body style-v1">
    <!-- Header -->
    <header class="site-header header-s1 is-sticky">
        <!-- Navbar -->
        <div class="navbar navbar-primary">
            <div class="container">
                <!-- Logo -->
                <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                    <img class="logo logo-dark" alt="" src="~/image/logo.png" srcset="image/logo2x.png 2x">
                    <img class="logo logo-light" alt="" src="~/image/logo-white.png" srcset="image/logo-white2x.png 2x">
                </a>
                <!-- #end Logo -->
                <!-- HeaderTop -->
                <div class="header-top">
                    <ul class="top-contact">
                        <li class="t-phone t-phone1 t-phone-s2">
                            <span><small><em class="color-secondary">24H</em> SERVICE NUMBER</small> <em class="fa fa-phone" aria-hidden="true"></em> <a href="#">076 573 8486</a></span>
                        </li>
                        <li class="t-phone t-phone1 t-phone-s2">
                            <span><small>CALL OFFICE <em class="color-secondary"> #</em> </small> <em class="fa fa-phone" aria-hidden="true"></em> <a href="#">011 246 8974 </a></span>
                        </li>
                        <li class="quote-btn"><a href="@Url.Action("Contact", "Home")" class="btn">Get In Touch Now</a></li>
                    </ul>

                    @Html.Partial("_LoginPartial")
                </div>
                <!-- #end HeaderTop -->
                <!-- NavBar Trigger -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#mainnav"
                            aria-expanded="false">
                        <span class="sr-only">Menu</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- Q-Button for Mobile -->
                    <div class="quote-btn"><a class="btn" href="@Url.Action("Contact", "Home")"><span>Request Service</span></a></div>
                </div>
                <!-- #end Trigger -->
                <!-- MainNav -->
                <nav class="navbar-collapse collapse" id="mainnav">
                    <ul class="nav navbar-nav">
                        <li>

                            <a href="@Url.Action("Index", "Home")">Home</a>
                        </li>

                        <li>
                            <a href="@Url.Action("About", "Home")">About</a>
                        </li>
                        <li>
                            <a href="#">Products</a>
                            <ul>
                                <li><a href="@Url.Action("AL8000", "Home")">AL8000</a></li>
                                <li><a href="@Url.Action("AL9010", "Home")">AL9010</a></li>
                                <li><a href="@Url.Action("ALC1", "Home")">ALC1</a></li>
                                <li><a href="@Url.Action("ALP1", "Home")">ALP1</a></li>
                                <li><a href="@Url.Action("EBS", "Home")">EBS</a></li>
                                <li><a href="@Url.Action("EBS010", "Home")">EBS010</a></li>
                            </ul>
                        </li>

                        <li>
                            <a href="@Url.Action("Contact", "ContactInfos")">Contact</a>

                        </li>
                        @if (Request.IsAuthenticated && User.IsInRole("Admin"))
                        {
                            <li>
                                <a href="#">Administration</a>
                                <ul>
                                    <li>@Html.ActionLink("Roles Admin", "Index", "RolesAdmin")</li>
                                    <li>@Html.ActionLink("Users Admin", "Index", "UsersAdmin")</li>

                                </ul>
                            </li>

                            <li>
                                <a href="#">Messages</a>
                                <ul>
                                    <li>
                                        <a href="@Url.Action("Index", "ScheduleServices")">Quick Contact</a>
                                    </li>

                                    <li>
                                        <a href="@Url.Action("Index", "ContactInfos")">Contact Messages</a>
                                    </li>

                                </ul>
                            </li>
                        }

                    </ul>
                </nav>
                <!-- #end MainNav -->
            </div>
        </div>
        <!-- #end Navbar -->

    </header>
    <!-- End Header -->
    @RenderSection("featured", required: false)
    <!-- Content -->
    <div class="section section-contents section-pad">
        <div class="container">
            <div class="content row">
                <div class="row">
                    <div class="col-md-8">

                        @RenderBody()


                    </div>
                    <!-- Sidebar -->
                    <div class="col-md-4">
                        <div class="sidebar-right">
                            <div class="wgs-box wgs-menus">
                                <div class="wgs-content">
                                    <ul class="list list-grouped">
                                        <li class="list-heading">
                                            <span>About Tembisa Property Stokvel</span>
                                            <ul>
                                                <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                                                <li class="current"><a href="@Url.Action("About", "Home")">About Company</a></li>
                                               
                                            </ul>
                                        </li>
                                        <li><a href="@Url.Action("Contact", "Home")"><span>Contacts</span></a></li>

                                    </ul>
                                </div>
                            </div>

                            <div class="wgs-box boxed bg-secondary light boxed-flat">
                                <div class="wgs-content">
                                    <h5>24HR EMERGENCY SERVICE</h5>
                                    <p>A team of professionals who are there when you need them, Just call.</p>
                                    <a href="@Url.Action("Contact", "Home")" class="btn btn-light btn-alt">Request A Quote</a>
                                </div>
                            </div>

                        </div>
                    </div>
                    <!-- Sidebar #end -->
                </div>
            </div>
        </div>
    </div>
    <!-- End content -->

    <!-- Call Action -->
    <div class="call-action bg-primary">
        <div class="cta-block">
            <div class="container">
                <div class="content row">
                    <div class="cta-sameline">
                        <h3>Complete breathalyzer solution.</h3>
                        <a class="btn btn-outline" href="@Url.Action("Contact", "Home")">Get A Quote</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Section -->
    <!-- Footer Widget-->
    <div class="footer-widget section-pad-md">
        <div class="container">
            <div class="row">
                <div class="widget-row row">
                    <div class="footer-col col-md-3 col-sm-6 res-m-bttm">
                        <!-- Each Widget -->
                        <div class="wgs wgs-footer wgs-menu">
                            <h5 class="wgs-title">Our Breathalyzers</h5>


                            <div class="wgs-content">
                                <ul class="menu">
                                    <li><a href="@Url.Action("Products", "Home")">EBS-010</a></li>
                                    <li><a href="@Url.Action("Products", "Home")">ALC-1</a></li>
                                    <li><a href="@Url.Action("Products", "Home")">ALP-1</a></li>
                                    <li><a href="@Url.Action("Products", "Home")">AL8000</a></li>
                                    <li><a href="@Url.Action("Products", "Home")">EBS</a></li>
                                    <li><a href="@Url.Action("Products", "Home")">AL9010</a></li>
                                </ul>
                            </div>
                        </div>
                        <!-- End Widget -->
                    </div>
                    <div class="footer-col col-md-3 col-sm-6 res-m-bttm">
                        <!-- Each Widget -->
                        <div class="wgs wgs-footer wgs-menu">
                            <h5 class="wgs-title">Quick Links</h5>
                            <div class="wgs-content">
                                <ul class="menu">
                                    <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                                    <li><a href="@Url.Action("About", "Home")">About Us</a></li>
                                 

                                    <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
                                </ul>
                            </div>
                        </div>
                        <!-- End Widget -->
                    </div>
                    <div class="footer-col col-md-3 col-sm-6 res-m-bttm">
                        <!-- Each Widget -->
                        <div class="wgs wgs-footer">
                            <h5 class="wgs-title">Company Information</h5>
                            <div class="wgs-content">
                                <ul class="contact-info">
                                    <li>
                                        <span>Cell</span>:076 573 8486<br>
                                        <span>Office</span>: 011 246 8974
                                    </li>
                                    <li>
                                        27 Commissioner Street, Kempton Park <br>
                                        Gauteng, South Africa.
                                    </li>
                                    <li>Giving excellent service to South Africa  &amp; the rest of the continent of Africa.</li>
                                </ul>
                                <ul class="social social-v2">
                                    <li><a href="#"><em class="fa fa-facebook" aria-hidden="true"></em></a></li>
                                    <li><a href="#"><em class="fa fa-twitter" aria-hidden="true"></em></a></li>
                                    <li><a href="#"><em class="fa fa-linkedin" aria-hidden="true"></em></a></li>
                                </ul>
                            </div>
                        </div>
                        <!-- End Widget -->
                    </div>
                    <div class="footer-col col-md-3 col-sm-6">
                        <!-- Each Widget -->
                        <div class="wgs wgs-footer">
                            <div class="wgs-content">
                                <h5 class="wgs-title">About Tembisa Property Stokvel</h5> <br />
                                <div class="footer-logo"><img src="~/image/logo-white.png" srcset="~/image/logo-white2x.png 2x" alt=""></div>

                                <p>Tembisa Property Stokvel embraces international standards of alcohol elimination and control on the roads and as an integral part of its mission is geared towards that objective.</p>
                            </div>
                        </div>
                        <!-- End Widget -->
                    </div>
                </div><!-- Widget Row -->
            </div>
        </div>
    </div>
    <!-- End Footer Widget -->
    <!-- Copyright -->
    <div class="copyright">
        <div class="container">
            <div class="row">
                <div class="site-copy col-sm-7">
                    <p>Copyright &copy; 2017 Tembisa Property Stokvel. All Rights Reserved <span class="sep"> . </span> </p>
                </div>
                <div class="site-by col-sm-5 al-right">
                    <p>Created By <a href="https://fhatuwasystems.co.za/" target="_blank">Fhatuwa Systems</a></p>
                </div>
            </div>
        </div>
    </div>
    <!-- End Copyright -->
    <!-- JavaScript Bundle -->
    <!-- Google Map Script -->
    <script src="https://maps.google.com/maps/api/js?key=AIzaSyDNaa8LOQISEST6NIGC78vmdnIOynR2SeU"></script>
    <script src="js/gmaps.js"></script>


    <script src="js/jquery.bundle.js"></script>
    <!-- Theme Script init() -->
    <script src="js/script.js"></script>
    <!-- End script -->
    @Scripts.Render("~/https://maps.google.com/maps/api/js?key=AIzaSyDNaa8LOQISEST6NIGC78vmdnIOynR2SeU")
    @Scripts.Render("~/js/gmaps.js")
    @Scripts.Render("~/js/jquery.bundle.js")
    @Scripts.Render("~/js/script.js")



</body>
</html>



